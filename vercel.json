      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Cafeter√≠a App",
    "APP_ENV": "production",
    "APP_KEY": "base64:xI6RUJ7eqcM4Yr+/0drwrCscaRA/8w7mPLmcbBUaXcA=",
    "APP_DEBUG": "false",
    "APP_URL": "https://your-app.vercel.app",
    "LOG_CHANNEL": "stderr",
